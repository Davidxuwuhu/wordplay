{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noUnusedLocals": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "baseUrl": "src",
        "paths": {
            "@components/*": ["components/*"],
            "@nodes/*": ["nodes/*"],
            "@runtime/*": ["runtime/*"],
            "@conflicts/*": ["conflicts/*"],
            "@translation/*": ["translation/*"],
            "@input/*": ["input/*"],
            "@output/*": ["output/*"],
            "@concepts/*": ["concepts/*"],
            "@parser/*": ["parser/*"],
            "@native/*": ["native/*"],
            "@transforms/*": ["transforms/*"],
            "@models/*": ["models/*"],
            "@db/*": ["db/*"]
        }
    }
}
