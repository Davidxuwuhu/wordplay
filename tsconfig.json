{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noUnusedLocals": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "baseUrl": "src",
        "paths": {
            "@components/*": ["components/*"],
            "@nodes/*": ["nodes/*"],
            "@runtime/*": ["runtime/*"],
            "@conflicts/*": ["conflicts/*"],
            "@locale/*": ["locale/*"],
            "@input/*": ["input/*"],
            "@output/*": ["output/*"],
            "@concepts/*": ["concepts/*"],
            "@parser/*": ["parser/*"],
            "@basis/*": ["basis/*"],
            "@edit/*": ["edit/*"],
            "@models/*": ["models/*"],
            "@db/*": ["db/*"]
        }
    }
}
